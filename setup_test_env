#!/bin/bash
. /usr/local/bin/test_lib.sh

try cd "$TEST_DIR"
rm -rf "$PROJECT"
try git clone https://github.com/glideinWMS/glideinwms.git
#ci_branch="dbox_ci"
ci_branch="master"
if [ "$1" = "" ]; then
    branch="$ci_branch"
else
    branch=$1
fi
try cd "$TEST_DIR/$PROJECT"
if [ "$branch" != "$ci_branch" ]; then
  try git checkout "$branch"
fi
try cd build/jenkins
for F in $TEST_DIR/util/* ; do
  try cp "$F" .
done
